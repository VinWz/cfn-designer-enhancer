{
  "name": "Cloudformation Designer Enhancer",
  "version": "0.0.1",
  "description": "Chrome extension to add Cloudformation Resource documentation and fillable form fields to Cloudformation designer view.",
  "devDependencies": {
    "brfs": "^1.4.0",
    "browserify": "^9.0.3",
    "gif.js": "~0.1.6",
    "gulp": "^3.8.10",
    "gulp-less": "~1.3.1",
    "gulp-util": "^3.0.2",
    "lessify": "^1.0.0",
    "reactify": "^1.0.0",
    "simple-mock": "^0.2.8",
    "tape": "^3.5.0",
    "vinyl-source-stream": "~0.1.1",
    "watchify": "^2.4.0",
    "zuul": "^3.2.0"
  },
  "dependencies": {
    "classnames": "^1.1.2",
    "lodash": "^3.2.0",
    "react": "^0.12.2"
  },
  "browserify": {
    "debug": true,
    "transform": [
      "reactify",
      "lessify",
      "brfs"
    ]
  },
  "scripts": {
    "build": "gulp build",
    "dev": "gulp dev",
    "test": "zuul -- test/**/*{.js,.jsx}",
    "test:browser": "zuul --local 55555 -- test/**/*{.js,.jsx}"
  }
}
